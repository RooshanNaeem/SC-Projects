    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "^16.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tslint -c tslint.json --fix",
      "git add"
    ],
    "client/src/**/*.ts": [
      "tslint -c client/src/tslint.json --fix",
      "git add"
    ]
  },
  "nodemonConfig": {
    "verbose": true,
    "ext": "ts",
    "watch": [
      "./src/",
      "./common/"
    ]
  }
}
